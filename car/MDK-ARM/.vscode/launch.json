{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Arm Debugger",
      "type": "arm-debugger",
      "request": "launch",
      "serialNumber": "${command:device-manager.getSerialNumber}",
      "program": "${command:arm-debugger.getApplicationFile}",
      "cmsisPack": "${command:device-manager.getDevicePack}",
      "deviceName": "${command:cmsis-csolution.getDeviceName}",
      "processorName": "${command:cmsis-csolution.getProcessorName}"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "e:/Desktop/f32xuexi/car/Core/Src",
      "program": "e:/Desktop/f32xuexi/car/Core/Src/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}